<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <parent>
        <artifactId>api-boot-maven-plugins</artifactId>
        <groupId>org.minbox.framework</groupId>
        <version>${revision}</version>
    </parent>
    <modelVersion>4.0.0</modelVersion>
    <packaging>maven-plugin</packaging>
    <properties>
        <main.basedir>${basedir}/../../..</main.basedir>
        <mysql-connector-java.version>8.0.28</mysql-connector-java.version>
    </properties>
    <artifactId>api-boot-mybatis-enhance-maven-codegen</artifactId>
    <dependencies>
        <!--Codegen-->
        <dependency>
            <groupId>com.mysema.codegen</groupId>
            <artifactId>codegen</artifactId>
        </dependency>
        <!--Code Builder-->
        <dependency>
            <groupId>com.gitee.hengboy</groupId>
            <artifactId>code-builder-core</artifactId>
        </dependency>
        <!--Mybatis Enhance Core-->
        <dependency>
            <groupId>com.gitee.hengboy</groupId>
            <artifactId>mybatis-enhance-core</artifactId>
            <optional>true</optional>
        </dependency>
        <!--Mybatis Enhance Dsl-->
        <dependency>
            <groupId>com.gitee.hengboy</groupId>
            <artifactId>mybatis-enhance-dsl</artifactId>
            <optional>true</optional>
        </dependency>
        <!--Lombok-->
        <dependency>
            <groupId>org.projectlombok</groupId>
            <artifactId>lombok</artifactId>
            <optional>true</optional>
        </dependency>
        <!--fastJson-->
        <dependency>
            <groupId>com.alibaba</groupId>
            <artifactId>fastjson</artifactId>
        </dependency>
        <dependency>
            <groupId>mysql</groupId>
            <artifactId>mysql-connector-java</artifactId>
            <version>${mysql-connector-java.version}</version>
            <optional>true</optional>
        </dependency>
        <!--Ssh Agent-->
        <dependency>
            <groupId>org.minbox.framework</groupId>
            <artifactId>ssh-agent</artifactId>
            <version>${ssh-agent.version}</version>
            <optional>true</optional>
        </dependency>
    </dependencies>

</project>
