<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <parent>
        <artifactId>api-boot-build</artifactId>
        <groupId>org.minbox.framework</groupId>
        <version>${revision}</version>
    </parent>
    <modelVersion>4.0.0</modelVersion>
    <properties>
        <!--跳过test-->
        <maven.test.skip>true</maven.test.skip>
        <!--跳过javadoc生成-->
        <maven.javadoc.skip>true</maven.javadoc.skip>
        <!--跳过deploy-->
        <maven.deploy.skip>true</maven.deploy.skip>
        <!--Samples所使用的ApiBoot版本-->
        <api-boot.version>${project.version}</api-boot.version>
        <!--Samples所使用的SpringBoot版本-->
        <spring-boot.version>2.3.1.RELEASE</spring-boot.version>
    </properties>
    <artifactId>api-boot-samples</artifactId>
    <packaging>pom</packaging>
    <modules>
        <module>api-boot-sample-http-converter</module>
        <module>api-boot-sample-security-oauth-jwt</module>
        <module>api-boot-sample-swagger</module>
        <module>api-boot-sample-alibaba-oss</module>
        <module>api-boot-sample-alibaba-sms</module>
        <module>api-boot-sample-quartz</module>
        <module>api-boot-sample-datasource-switch</module>
        <module>api-boot-sample-resource-load</module>
        <module>api-boot-sample-message-push</module>
        <module>api-boot-sample-rate-limiter</module>
        <module>api-boot-sample-mybatis-pageable</module>
        <module>api-boot-sample-mybatis-enhance</module>
        <module>api-boot-sample-mybatis-enhance-codegen</module>
        <module>api-boot-sample-integration</module>
        <module>api-boot-sample-mail</module>
        <module>api-boot-sample-logging</module>
        <module>api-boot-sample-logging-admin</module>
        <module>api-boot-sample-sequence</module>
        <module>api-boot-sample-mongo-client-settings</module>
        <module>api-boot-sample-tools</module>
        <module>api-boot-sample-message-pipe-client</module>
        <module>api-boot-sample-message-pipe-server</module>
    </modules>
</project>
